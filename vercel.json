{
    "version": 2,
    "builds": [
      {
        "src": "pom.xml",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "target"
        },
        "buildCommand": "mvn clean install"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/target/fmjavaserver-0.0.1-SNAPSHOT.jar"
      }
    ]
  }
  